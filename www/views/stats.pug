extends main
block content
    title Stats - Okey bot
    div(class="center")
        table
            tr 
                td Uptime 
                td #{uptime}
            tr 
                td RAM Usage 
                td #{ram}mb
            tr
                td Available commands
                td #{commands}
            tr
                td Active Channels
                td #{channelCount}
            tr 
                td Issued commands since last bot restart
                td #{issuedCommands}
            tr 
                td All ever issued commands
                td #{ALLissuedCommands}